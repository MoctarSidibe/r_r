{
  "service": {
    "id": "rabbitmq",
    "name": "rabbitmq",
    "tags": ["message-queue", "rabbitmq", "amqp"],
    "port": 5672,
    "address": "rabbitmq",
    "check": {
      "script": "rabbitmq-diagnostics ping",
      "interval": "30s",
      "timeout": "10s",
      "deregister_critical_service_after": "30s"
    },
    "meta": {
      "version": "3.12",
      "environment": "production",
      "management_port": "15672",
      "protocol": "amqp"
    }
  }
}
