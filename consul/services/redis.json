{
  "service": {
    "id": "redis",
    "name": "redis",
    "tags": ["cache", "redis"],
    "port": 6379,
    "address": "redis",
    "check": {
      "script": "redis-cli ping",
      "interval": "10s",
      "timeout": "3s",
      "deregister_critical_service_after": "30s"
    },
    "meta": {
      "version": "7",
      "environment": "production"
    }
  }
}
